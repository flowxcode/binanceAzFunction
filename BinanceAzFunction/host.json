{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Warning", // Keeps overall noise low
      "Function.Function1": "Information", // Allows system logs (Executing, Trigger Details, Executed)
      "Function.Function1.User": "Information" // Allows your custom logs
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": false // Disable sampling entirely for testing
      },
      "enableLiveMetricsFilters": true
    }
  }
}